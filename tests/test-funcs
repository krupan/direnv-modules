# -*- sh -*-
end_test () {
    direnv deny .
    rm -f .envrc
    case $2 in
        "pass")
            echo "$1 pass"
            exit 0
            ;;
        "fail")
            echo "$1 FAIL!"
            exit 1
            ;;
        *)
            echo "end_test error: please call with pass or fail"
            exit 2
    esac
}
